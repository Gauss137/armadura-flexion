{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/steel-calculator",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-project-name",
          "value": "steel-calculator"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/steel-calculator",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-project-name",
          "value": "steel-calculator"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/balasto-calculator",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-project-name",
          "value": "balasto-calculator"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/balasto-calculator",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-project-name",
          "value": "balasto-calculator"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/vigas/simplemente-apoyadas/carga-uniforme",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-project-name",
          "value": "beam-calculator"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/vigas/simplemente-apoyadas/carga-uniforme",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-project-name",
          "value": "beam-calculator"
        }
      ]
    }
  ]
} 